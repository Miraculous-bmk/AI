{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/aria": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "css-prefix-order": "off"
  }
}